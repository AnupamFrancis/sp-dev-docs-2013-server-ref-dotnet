      <MemberSignature Language="F#" Value="member this.MetaTagPagePropertyMappings : System.Collections.Generic.Dictionary&lt;string, string&gt; with get, set" Usage="Microsoft.SharePoint.Publishing.AddinSettings.MetaTagPagePropertyMappings" />
      <MemberType>Property</MemberType>
      <AssemblyInfo>
